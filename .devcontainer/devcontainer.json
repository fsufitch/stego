{
    "name": "Stego Devcontainer",
    "build": {
        "dockerfile": "./Containerfile",
        "context": "."
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/developer/stego,type=bind,consistency=cached",
    "workspaceFolder": "/home/developer/stego",

    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "msyrus.go-doc",
                "wdhongtw.gpg-indicator",
                "yzhang.markdown-all-in-one",
                "zxh404.vscode-proto3",
                "foxundermoon.shell-format"
            ]
        }
    }
}